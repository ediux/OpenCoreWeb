@model IEnumerable<My.Core.Infrastructures.Implementations.Models.ApplicationRole>

@{
    ViewBag.Title =   OpenCoreWeb.MUI.PageMUI.RoleDashboard;
}

<h2>@ViewBag.Title</h2>

<p>
    @Html.ActionLink(OpenCoreWeb.MUI.ControlMUI.CreateRole, "Create") | @Html.ActionLink(OpenCoreWeb.MUI.ControlMUI.RoleAssign, "Index", "UserRoles") | @Html.ActionLink(OpenCoreWeb.MUI.ControlMUI.BackToManage, "Index", "Manage")
</p>
<p>
    
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Void)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreateUserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreateTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastUpdateUserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastUpdateTime)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Void)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreateUserId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreateTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastUpdateUserId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastUpdateTime)
        </td>
        <td>
            @Html.ActionLink(OpenCoreWeb.MUI.ControlMUI.Edit, "Edit", new { id = item.Id }) |
            @Html.ActionLink(OpenCoreWeb.MUI.ControlMUI.Details, "Details", new { id = item.Id }) |
            @Html.ActionLink(OpenCoreWeb.MUI.ControlMUI.Delete, "Delete", new { id = item.Id })
        </td>
    </tr>
}

</table>
